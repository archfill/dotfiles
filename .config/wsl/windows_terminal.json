{
    "$help": "https://aka.ms/terminal-documentation",
    "$schema": "https://aka.ms/terminal-profiles-schema",
    "actions": [
        {
            "command": {
                "action": "copy",
                "singleLine": false
            },
            "keys": "ctrl+c"
        },
        {
            "command": "paste",
            "keys": "ctrl+v"
        },
        {
            "command": "find",
            "keys": "ctrl+shift+f"
        },
        {
            "command": {
                "action": "splitPane",
                "split": "auto",
                "splitMode": "duplicate"
            },
            "keys": "alt+shift+d"
        }
    ],
    "copyFormatting": "none",
    "copyOnSelect": false,
    "defaultProfile": "{arch-linux-guid}",
    "profiles": {
        "defaults": {
            "antialiasingMode": "grayscale",
            "colorScheme": "Campbell Powershell",
            "cursorShape": "bar",
            "font": {
                "face": "HackGen Console NF",
                "size": 12
            },
            "opacity": 95,
            "useAcrylic": true
        },
        "list": [
            {
                "name": "Arch Linux",
                "guid": "{arch-linux-guid}",
                "commandline": "wsl.exe -d Arch",
                "startingDirectory": "//wsl$/Arch/home/%USERNAME%",
                "icon": "ms-appx:///ProfileIcons/linux.png",
                "colorScheme": "Campbell",
                "font": {
                    "face": "HackGen Console NF",
                    "size": 12
                }
            },
            {
                "name": "Ubuntu",
                "guid": "{ubuntu-guid}",
                "commandline": "wsl.exe -d Ubuntu",
                "startingDirectory": "//wsl$/Ubuntu/home/%USERNAME%",
                "icon": "ms-appx:///ProfileIcons/ubuntu.png",
                "colorScheme": "Ubuntu",
                "font": {
                    "face": "HackGen Console NF",
                    "size": 12
                }
            }
        ]
    },
    "schemes": [
        {
            "name": "Ubuntu",
            "black": "#2e3436",
            "red": "#cc0000",
            "green": "#4e9a06",
            "yellow": "#c4a000",
            "blue": "#3465a4",
            "purple": "#75507b",
            "cyan": "#06989a",
            "white": "#d3d7cf",
            "brightBlack": "#555753",
            "brightRed": "#ef2929",
            "brightGreen": "#8ae234",
            "brightYellow": "#fce94f",
            "brightBlue": "#729fcf",
            "brightPurple": "#ad7fa8",
            "brightCyan": "#34e2e2",
            "brightWhite": "#eeeeec",
            "background": "#300a24",
            "foreground": "#eeeeec",
            "selectionBackground": "#b5d5ff",
            "cursorColor": "#bbbbbb"
        }
    ],
    "themes": [
        {
            "name": "dark",
            "tab": {
                "background": "#0C0C0C",
                "showCloseButton": "always",
                "unfocusedBackground": "#333333"
            },
            "window": {
                "applicationTheme": "dark",
                "useMica": false
            }
        }
    ]
}