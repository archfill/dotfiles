{
  "view用page": {
    "body": [
      "// ignore: unused_import, directives_ordering",
      "import 'package:flutter_riverpod/flutter_riverpod.dart';",
      "import 'package:flutter/material.dart';",
      "",
      "import '../view_model/${TM_FILENAME_BASE}_notifier.dart';",
      "",
      "class ${1:${TM_FILENAME_BASE/(.*)/${1:/pascalcase}/g}} extends ConsumerWidget {",
      "  const ${1}({Key? key}) : super(key: key);",
      "",
      "  @override",
      "  Widget build(BuildContext context, WidgetRef ref) {",
      "    // state（状態）",
      "    final ${2:${TM_FILENAME_BASE/(.*)/${1:/camelcase}/g}}State = ref.watch(${2}Provider);",
      "    // provider（状態の操作）",
      "    final ${2}Notifier = ref.watch(${2}Provider.notifier);",
      "    return Scaffold(",
      "      appBar: AppBar(title: const Text('${1}')),",
      "      floatingActionButton: FloatingActionButton(",
      "        onPressed: ${2}Notifier.resetAllCount,",
      "        child: const Icon(Icons.exposure_zero),",
      "      ),",
      "      body: ListView(",
      "        children: [",
      "          ListTile(",
      "            title: Text('Main Count \\${${2}State.mainCount}'),",
      "            onTap: ${2}Notifier.increaseMainCount,",
      "          ),",
      "          ListTile(",
      "            title: Text('Sub Count \\${${2}State.subCount}'),",
      "            onTap: ${2}Notifier.increaseSubCount,",
      "          ),",
      "        ],",
      "      ),",
      "    );",
      "  }",
      "}",
      ""
    ],
    "prefix": ["mvvm-view"],
    "description": "mvvm base page"
  },
  "ModelのState用Freezed": {
    "body": [
      "// ignore: unused_import, directives_ordering",
      "import 'package:flutter/foundation.dart';",
      "import 'package:freezed_annotation/freezed_annotation.dart';",
      "",
      "part '$TM_FILENAME_BASE.freezed.dart';",
      "",
      "@freezed",
      "class ${1:${TM_FILENAME_BASE/(.*)/${1:/pascalcase}/g}} with _$${1} {",
      "  const factory ${1}({",
      "    // required ${2:String id},",
      "    @Default(0) int mainCount,",
      "    @Default(0) int subCount,",
      "  }) = _${1};",
      "}"
    ],
    "prefix": ["mvvm-model"],
    "description": "mvvm base model"
  },
  "viewmodel用notifier": {
    "body": [
      "// ignore: unused_import, directives_ordering",
      "import 'package:flutter_riverpod/flutter_riverpod.dart';",
      "",
      "import '../model/${1:${TM_FILENAME_BASE/(.*)_notifier/$1/g}}_state.dart';",
      "",
      "class ${2:${TM_FILENAME_BASE/(.*)_notifier/${1:/pascalcase}/g}}Notifier extends StateNotifier<${2}State> {",
      "  // 初期値の指定",
      "  ${2}Notifier() : super(const ${2}State());",
      "",
      "  // メインカウントを+1する",
      "  void increaseMainCount() async {",
      "    state = state.copyWith(mainCount: state.mainCount + 1);",
      "  }",
      "",
      "  // サブカウントを+1する",
      "  void increaseSubCount() async {",
      "    state = state.copyWith(subCount: state.subCount + 1);",
      "  }",
      "",
      "  // すべてのカウントを0に戻す",
      "  void resetAllCount() async {",
      "    state = state.copyWith(",
      "      mainCount: 0,",
      "      subCount: 0,",
      "    );",
      "  }",
      "}",
      "",
      "// HomePageNotifierの状態を管理する",
      "final ${3:${TM_FILENAME_BASE/(.*)_notifier/${1:/camelcase}/g}}Provider =",
      "    StateNotifierProvider.autoDispose<${2}Notifier, ${2}State>(",
      "  (ref) => ${2}Notifier(),",
      ");",
      ""
    ],
    "prefix": ["mvvm-viewmodel"],
    "description": "mvvm base viewmodel"
  }
}
